      "@spartan-ng/helm/utils": [
        "./libs/ui/utils/src/index.ts"
      ],
      "@/*": [
        "./src/*"
      ],
      "@environment": [
        "./src/environments/environment"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@components/*": [
        "./src/app/components/*"
      ],
      "@models/*": [
        "./src/app/entities/models/*"
      ],
      "@entities/*": [
        "./src/app/entities/*"
      ],
      "@daos/*": [
        "./src/app/daos/*"
      ],
      "@helpers/*": [
